<% results.forEach(function(result) { %>
  <a style="display: block; height: 100%; text-decoration: none; color: #212529;" href="/products/<%= result.CategoryKey %>/items/<%= result.ProductKey %>">
    <div class="card itemCard">
        <div class="card-header">
          <h1 style="color: white;"><%= result.Title %></h1>
          <img style="width: 250px;" src="<%= result.Image %>"></img>
          <p><%= result.Description %></p>
      </div>
    </div>
  </a>
<% }); %>
