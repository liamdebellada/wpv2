<body style="background: #020024;
background: linear-gradient(90deg, #020024 0%, #1d2237 0%, #3c346b); font-family: 'Raleway', sans-serif; padding-top: 70px;">


    <%- include('navbar.ejs') %>       
    <div></div>
    

    
