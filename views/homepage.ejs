<% results.forEach(function(result) { %>
  <a style="display: block; height: 100%; text-decoration: none; color: #212529;" href="/products/<%= result.CategoryKey %>">
    <div class="card itemCard" style="text-align: center;">
        <div class="card-header">
          <h1 style="color: white;"><%= result.CategoryKey %></h1>
          <img style="width:250px;" src="<%= result.Image %>"></img>
          <p><%= result.Description %></p>
      </div>
    </div>
  </a>
<% }); %>
