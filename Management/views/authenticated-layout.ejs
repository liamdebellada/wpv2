        function breadCrumbsDisplay(desktopDisplay, mobileDisplay) {
    $('#wp-breadcrumbs').html("")
    

    if(window.innerWidth > 768) {

      desktopDisplay.forEach(function(listItem) {
        if (listItem.href != "") {
          $('#wp-breadcrumbs').append(`<li class="breadcrumb-item"><a href="${listItem.href}">${listItem.title}</a></li>`)
        } else {
          $('#wp-breadcrumbs').append(`<li class="breadcrumb-item">${listItem.title}</li>`)
        }
      })
        
      
    
    } else { //you got smartph owned nooob!
      $('#wp-breadcrumbs').html(`<li class="breadcrumb-item" aria-current="${mobileDisplay.href}"><a href="/dashboard">${mobileDisplay.title}</a></li>`)
    }
  }
