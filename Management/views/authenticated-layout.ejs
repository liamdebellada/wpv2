<script>
  var counterBack
  var i;
  function progressBarStart() {
    i = 10
    $('#wp-progress-bar').collapse('show')
      counterBack = setInterval(function(){
        i+= 10;
        if (i < 95 && $('#wp-progress-bar-time > .progress-bar').css('width') != "100%"){
          $('#wp-progress-bar-time > .progress-bar').css('width', i+'%');
        } else {
          clearInterval(counterBack);
        }
        
      }, 300);
  }
  function progressBarComplete(requireReload = false) {
    var progressPromise = new Promise((resolve, reject) => {
    setTimeout(() => {
          clearInterval(counterBack); 
          $('#wp-progress-bar-time > .progress-bar').css('width', 100+'%')
          $('#wp-progress-bar-time > .progress-bar').addClass('bg-success');
          setTimeout(() => {
            if (!requireReload) {
              $('#wp-progress-bar').collapse('hide').on('hidden.bs.collapse', function () { 
              $('#wp-progress-bar-time > .progress-bar').css('width', 0+'%')
              $('#wp-progress-bar-time > .progress-bar').removeClass('bg-success');
              resolve("complete")
          })
            } else {
              $('#wp-progress-bar-reload').collapse('show').on('shown.bs.collapse', function () {
                resolve("complete")
              })
            }
            
          }, 1000);
          
        }, 1500);
  })
  return progressPromise 
  }
  
</script>